# Environment variables for Canvas LMS Student Dashboard
# Copy this file to .env.local and fill in your actual values

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your-supabase-project-url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

# Encryption Key for Canvas Tokens (change this to a secure random string)
NEXT_PUBLIC_ENCRYPTION_KEY=your-secure-encryption-key-here

# Optional: Canvas API Rate Limiting
CANVAS_API_RATE_LIMIT=10
